{
  "manifest_version": 2,

  "name": "Credential Switcher",
  "description": "Swap bp credentials",
  "version": "1.0",

  "content_scripts": [{
    "matches": ["http://localhost:9000/*"],
    "js": ["console.js"]
  }],

  "permissions": [
    "tabs",
    "activeTab",
    "background",
    "storage"
  ]
}
